{
	"definitions": {
		"CarrierCode": {
			"description": "IATA airline code, e.g. YY",
			"type": "string",
			"minLength": 2,
			"maxLength": 2,
			"pattern": "^[A-Z0-9]{2}"
		},
		"CityCode": {
			"description": "City/Airport IATA code, e.g. LON",
			"type": "string",
			"minLength": 3,
			"maxLength": 3,
			"pattern": "^[A-Z]{3}"
		},
		"CurrencyCode": {
			"description": "ISO currency code, e.g. USD",
			"type": "string",
			"minLength": 3,
			"maxLength": 3,
			"pattern": "^[A-Z]{3}"
		},
		"Date": {
			"description": "Date specified as YYYY-MM-DD",
			"type": "string",
			"minLength": 10,
			"maxLength": 10,
			"pattern": "^[1-9][0-9]{3}-[0-1][0-9]-[0-3][0-9]"
		},
		"Time": {
			"description": "Time specified as HH:MM:SS",
			"type": "string",
			"minLength": 8,
			"maxLength": 8,
			"pattern": "^[0-9]{2}:[0-9]{2}:[0-9]{2}"
		},
		"DateTime": {
			"description": "DateTime specified as YYYY-MM-DDTHH:MM:SS",
			"type": "string",
			"minLength": 19,
			"maxLength": 19,
			"pattern": "^[1-9][0-9]{3}-[0-1][0-9]-[0-3][0-9]T[0-9]{2}:[0-9]{2}:[0-9]{2}"
		},
		"Equipment": {
			"description": "Aircraft type",
			"type": "string",
			"minLength": 3,
			"maxLength": 3,
			"pattern": "^[0-9A-Z]{3}"
		}
	},
